    function trackScrollPosition(){
      const y = window.scrollY;
      const label = Math.min(Math.floor(y/30) + 1, 75);
      const imageToUse = plasmidImages[label];
      $('.plasmid').css('background-image', `url('${imageToUse}')`);
    }

    $(document).ready(()=>{
      $(window.scroll(()=>{
        trackScrollPosition();
      })
    })
  </script>
